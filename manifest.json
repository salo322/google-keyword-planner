{
    "name": "Keyword Planner Extension",
    "version": "1.1.5",
    "icons":{
      "16": "./img/copy16.png",
      "32":"./img/copy32.png",
      "64":"./img/copy64.png"
    },
    "description": "google keyword planner",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": {
        "16": "./img/copy16.png",
        "32":"./img/copy32.png",
        "64":"./img/copy64.png"
    }
    },
    "permissions": [
       "storage",
       "https://ads.google.com/aw/*",
       "contextMenus",
       "activeTab",
      "clipboard",
      "notifications"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["./js/jquery.js", "./js/content.js"],
          "css": ["./css/style.css"]
        }
      ],
    "background": {
      "scripts": ["./js/jquery.js", "./js/background.js"],
      "persistent": false

    }
  }
